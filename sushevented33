		local AnimationNew = Instance.new("Animation")
		AnimationNew.AnimationId = Id
		local Humanoid = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(AnimationNew)
		Humanoid:Play()
	end)
end
